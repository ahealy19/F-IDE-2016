goal WP_parameter_height1 "expl:VC for height1" : forall t:tree 'a. forall k:
  cont 'a, w:what 'a. ("stop_split" (evalk k
  match w with
  | Argument t1 -> height t1
  | Result x -> x
  end = height t)) ->
  ("split_goal: compiled match" match k with
  | Id -> true
  | _ -> false
  end) ->
  not not ("split_goal: compiled match" match w with
  | Result x -> true
  | _ -> false
  end) -> (forall x:int. (w = Result x:what 'a) ->
  ("expl:postcondition" (x = height t)))

