type tree 'a =
  | Empty
  | Node (tree 'a) 'a (tree 'a)

(* use bintree.Tree *)

function size (t:tree 'a) : int =
  match t with
  | Empty -> 0
  | Node l _ r -> infix_pl (infix_pl 1 (size l)) (size r)
  end

axiom size_nonneg : forall t:tree 'a. infix_lseq 0 (size t)

axiom size_empty : forall t:tree 'a. (0 = size t) <-> (t = Empty:tree 'a)

(* use bintree.Size *)

function height (t:tree 'a) : int =
  match t with
  | Empty -> 0
  | Node l _ r -> infix_pl 1 (max (height l) (height r))
  end

axiom height_nonneg : forall t:tree 'a. infix_lseq 0 (height t)

(* use bintree.Height *)

type ref 'a =
  | Mk_ref (contents:'a)

function prefix_ex (x:ref 'a) : 'a = contents x

(* use ref.Ref *)

type cont 'a =
  | Id
  | Kleft (tree 'a) (cont 'a)
  | Kright int (cont 'a)

type what 'a =
  | Argument (tree 'a)
  | Result int

predicate is_id (k:cont 'a) = match k with
  | Id -> true
  | _ -> false
  end

predicate is_result (w:what 'a) =
  match w with
  | Result _ -> true
  | _ -> false
  end

function evalk (k:cont 'a) (r:int) : int =
  match k with
  | Id -> r
  | Kleft l k1 -> evalk k1 (infix_pl 1 (max (height l) r))
  | Kright x k1 -> evalk k1 (infix_pl 1 (max x r))
  end

function evalw (w:what 'a) : int =
  match w with
  | Argument t -> height t
  | Result x -> x
  end

function sizek (k:cont 'a) : int =
  match k with
  | Id -> 0
  | Kleft t k1 -> infix_pl (infix_pl 3 (infix_as 4 (size t))) (sizek k1)
  | Kright _ k1 -> infix_pl 1 (sizek k1)
  end

axiom sizek_nonneg : forall k:cont 'a. infix_gteq (sizek k) 0

function sizew (w:what 'a) : int =
  match w with
  | Argument t -> infix_pl 1 (infix_as 4 (size t))
  | Result _ -> 0
  end

axiom helper1 : forall t:tree 'a. infix_gteq (infix_as 4 (size t)) 0

axiom sizew_nonneg : forall w:what 'a. infix_gteq (sizew w) 0

type tuple2 'a 'a1 =
