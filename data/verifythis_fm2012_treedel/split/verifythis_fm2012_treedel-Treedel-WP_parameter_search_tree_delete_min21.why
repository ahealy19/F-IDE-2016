  ("stop_split" let pt = Node subtree pp ppr in istree mem1 pp pt /\ (zip pt
  zipper = it)) -> not not (tt = null) -> ("asym_split" "stop_split"
  "expl:assertion" istree mem1 p subtree) -> ("asym_split"
