(* use int.Int *)

function length (l:list 'a) : int =
  match l with
  | Nil -> 0
  | Cons _ r -> infix_pl 1 (length r)
  end

axiom Length_nonnegative : forall l:list 'a. infix_gteq (length l) 0

axiom Length_nil : forall l:list 'a. (length l = 0) <-> (l = Nil:list 'a)

(* use list.Length *)

axiom Append_length : forall l1:list 'a, l2:list 'a. (length (infix_plpl l1
  l2) = infix_pl (length l1) (length l2))

predicate mem (x:'a) (l:list 'a) =
  match l with
  | Nil -> false
  | Cons y r -> (x = y) \/ mem x r
  end

(* use list.Mem *)

axiom mem_append : forall x:'a, l1:list 'a, l2:list 'a. mem x (infix_plpl l1
  l2) <-> mem x l1 \/ mem x l2

axiom mem_decomp : forall x:'a, l:list 'a. mem x l -> (exists l1:list 'a, l2:
  list 'a. (l = infix_plpl l1 (Cons x l2)))

(* use list.Append *)

function inorder (t:tree 'a) : list 'a =
  match t with
  | Empty -> Nil:list 'a
  | Node l x r -> infix_plpl (inorder l) (Cons x (inorder r))
  end

(* use bintree.Inorder *)

inductive distinct (list 'a) =
  | distinct_zero : distinct (Nil:list 'a)
  | distinct_one : forall x:'a. distinct (Cons x (Nil:list 'a))
  | distinct_many : forall x:'a, l:list 'a. not mem x l -> distinct l ->
      distinct (Cons x l)

axiom distinct_append : forall l1:list 'a, l2:list 'a. distinct l1 ->
  distinct l2 -> (forall x:'a. mem x l1 -> not mem x l2) -> distinct
  (infix_plpl l1 l2)

(* use list.Distinct *)

inductive istree (map loc node) loc (tree loc) =
  | leaf : forall m:map loc node. istree m null (Empty:tree loc)
  | node : forall m:map loc node, p:loc, l:tree loc, r:tree loc.
      not (p = null) -> istree m (left (mixfix_lbrb m p)) l -> istree m
      (right (mixfix_lbrb m p)) r -> istree m p (Node l p r)

type zipper 'a =
  | Top
  | Left (zipper 'a) 'a (tree 'a)

function zip (t:tree 'a) (z:zipper 'a) : tree 'a =
  match z with
  | Top -> t
  | Left z1 x r -> zip (Node t x r) z1
  end

axiom inorder_zip : forall z:zipper 'a, x:'a, l:tree 'a, r:tree 'a. (inorder
  (zip (Node l x r) z) = infix_plpl (inorder l) (Cons x (inorder (zip r z))))

axiom main_lemma : forall m:map loc node, t:loc, pp:loc, p:loc, ppr:tree loc,
  pr:tree loc, z:zipper loc.
  let it = zip (Node (Node (Empty:tree loc) p pr) pp ppr) z in istree m t
  it -> distinct (inorder it) ->
  (let mqt = mixfix_lblsmnrb m pp (let usq_ = mixfix_lbrb m pp in Mk_node
             (right (mixfix_lbrb m p)) (right usq_) (data usq_)) in istree
  mqt t (zip (Node pr pp ppr) z))

