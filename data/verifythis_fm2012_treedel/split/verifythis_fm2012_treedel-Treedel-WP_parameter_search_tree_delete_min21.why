  forall mem1:map loc node. ("stop_split" not (t = null)) /\
  ("stop_split" istree mem1 t it) /\ ("stop_split" distinct (inorder it)) ->
  ("asym_split" "stop_split" "expl:precondition" not (t = null)) -> not (left
  (get mem1 t) = null) -> (let o = left (get mem1 t) in ("asym_split"
  "stop_split" "expl:precondition" not (o = null)) -> ("asym_split"
  "stop_split" "expl:precondition" not (it = Empty:tree loc)) -> (forall o1:
  tree loc.
  ("stop_split" match it with
  | Empty -> false
  | Node _ _ r -> (o1 = r)
  end) -> ("asym_split" "stop_split" "expl:precondition" not (it = Empty:tree
  loc)) -> (forall o2:tree loc.
  ("stop_split" match it with
  | Empty -> false
  | Node l _ _ -> (o2 = l)
  end) -> (forall subtree:tree loc, ppr:tree loc, zipper:zipper loc, tt:loc,
  pp:loc, p:loc. ("stop_split" not (pp = null) /\ (left (get mem1
