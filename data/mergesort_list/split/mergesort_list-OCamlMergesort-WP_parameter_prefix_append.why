goal WP_parameter_prefix_append "why3:lemma"
  "expl:VC for prefix_append" : forall n:int, l1:list 'a, l2:list 'a.
  ("stop_split" infix_lseq (length l1) n /\ infix_lseq n (infix_pl (length
  l1) (length l2))) -> (l1 = Nil:list 'a) -> ("expl:postcondition" (prefix n
  (infix_plpl l1 l2) = infix_plpl (prefix (length l1) l1) (prefix (infix_mn n
  (length l1)) l2)))

