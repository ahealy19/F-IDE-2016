(* use bv.BV32 *)

function one3 : t17 = of_int 1

function two : t17 = of_int 2

function lastbit : t17 = sub size_bv one3

function max (x:t17) (y:t17) : t17 = if ult x y then y else x

function min (x:t17) (y:t17) : t17 = if ult x y then x else y

function count (bv:t17) : t17 =
  let x = sub bv (bw_and (lsr_bv bv one3) (of_int 0x55555555)) in
  let x1 = add (bw_and x (of_int 0x33333333)) (bw_and (lsr_bv x (of_int 2))
           (of_int 0x33333333)) in
  let x2 = bw_and (add x1 (lsr_bv x1 (of_int 4))) (of_int 0x0F0F0F0F) in
  let x3 = add x2 (lsr_bv x2 (of_int 8)) in
  let x4 = add x3 (lsr_bv x3 (of_int 16)) in bw_and x4 (of_int 0x0000003F)

function hammingD (a:t17) (b:t17) : t17 = count (bw_xor a b)

(* use Utils *)

predicate validAscii (b:t17) = (nth_bv (count b) zeros = False)

axiom asciiProp : forall a:t17, b:t17. validAscii a /\ not validAscii b \/
  validAscii b /\ not validAscii a <-> (nth_bv (hammingD a b) zeros = True)

function toGray (bv:t17) : t17 = bw_xor bv (lsr_bv bv one3)

function fromGray (gr:t17) : t17 =
  let b = bw_xor gr (lsr_bv gr (of_int 1)) in
  let b1 = bw_xor b (lsr_bv b (of_int 2)) in
  let b2 = bw_xor b1 (lsr_bv b1 (of_int 4)) in
  let b3 = bw_xor b2 (lsr_bv b2 (of_int 8)) in bw_xor b3 (lsr_bv b3 (of_int
  16))

axiom iso : forall b:t17. (toGray (fromGray b) = b) /\ (fromGray (toGray
  b) = b)

axiom grayIsGray : forall b:t17. ult b ones -> (hammingD (toGray b) (toGray
  (add b one3)) = one3)

