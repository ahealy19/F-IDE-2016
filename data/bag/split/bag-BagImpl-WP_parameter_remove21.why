goal WP_parameter_remove "expl:VC for remove" : forall t:int, t1:int, t2:map
  int (t17 'a), t3:func 'a int, i:int. ("asym_split" ("stop_split"
  "expl:type invariant" infix_lseq 0 t /\ (t = t1)) /\ ("stop_split"
  "expl:type invariant" forall i1:int. infix_lseq 0 i1 /\ infix_ls i1 t ->
  not match v (get t2 i1) with
  | Null _ -> true
  | Value _ -> false
  end) /\ ("stop_split" "expl:type invariant" forall x:'a. (infix_at t3
  x = numof1 (Mk_rarray t1 t2) x 0 t))) && ("stop_split" infix_lseq 0 i /\
  infix_ls i t) -> (let n = infix_mn t 1 in ("asym_split" "stop_split"
  "expl:precondition" infix_lseq 0 i /\ infix_ls i t1) ->
  (let o = get t2 i in ("asym_split" "stop_split"
  "expl:precondition" not match v o with
  | Null _ -> true
  | Value _ -> false
  end) -> (forall x:'a. ("stop_split" (v o = Value x)) -> not infix_ls i n ->
  ("asym_split" "stop_split" "expl:precondition" infix_lseq 0 n) ->
  (forall rho:map int (t17 'a), rho1:int. ("stop_split" (rho1 = n)) /\
  ("stop_split" forall i1:int. infix_lseq 0 i1 /\ infix_ls i1 t1 -> infix_ls
  i1 n -> (get rho i1 = get t2 i1)) -> (forall rho2:int. (rho2 = n) ->
  (forall rho3:func 'a int. (rho3 = remove x t3) -> ("asym_split"
  "stop_split" "expl:assertion" forall v1:'a. (numof1 (Mk_rarray rho1 rho) v1
  0 i = numof1 (Mk_rarray t1 t2) v1 0 i)) -> ("asym_split" "stop_split"
  "expl:assertion" forall v1:'a. (numof1 (Mk_rarray rho1 rho) v1 i n = numof1
  (Mk_rarray t1 t2) v1 (infix_pl i 1) (infix_pl n 1))) -> ("asym_split"
  "stop_split" "expl:assertion" forall v1:'a. (numof1 (Mk_rarray rho1 rho) v1
  0 n = infix_pl (numof1 (Mk_rarray rho1 rho) v1 0 i) (numof1 (Mk_rarray rho1
  rho) v1 i n))) -> ("asym_split" ("stop_split"
  "expl:type invariant" infix_lseq 0 rho2 /\ (rho2 = rho1)) /\ ("stop_split"
  "expl:type invariant" forall i1:int. infix_lseq 0 i1 /\ infix_ls i1 rho2 ->
  not match v (get rho i1) with
  | Null _ -> true
  | Value _ -> false
  end) /\ ("stop_split" "expl:type invariant" forall x1:'a. (infix_at rho3
  x1 = numof1 (Mk_rarray rho1 rho) x1 0 rho2))) ->
  ("expl:postcondition" forall x1:'a. (Value x1 = v (get t2 i)) -> infix_eqeq
  rho3 (remove x1 t3))))))))

