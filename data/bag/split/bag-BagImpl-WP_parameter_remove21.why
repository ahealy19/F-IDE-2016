type null = int

type t_model 'a =
  | Null int
  | Value 'a

type t17 'a =
  | Mk_t (v:t_model 'a)

predicate is_null (t:t17 'a) =
  match v t with
  | Null _ -> true
  | Value _ -> false
  end

(* use null.Null1 *)

function numof1 (r:rarray (t17 'a)) (x:'a) (l:int) (u:int) : int = numof
  (\ i:int. (v (get (data r) i) = Value x)) l u

type t18
  'a =
  | Mk_t1 (size:int) (data1:rarray (t17 'a)) (contents:func 'a int)

